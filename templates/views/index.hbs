{{!< default}}
{{# each data.sections}}
<div class="container full-width rounded">
	<div class="row">
		{{#if image.exists}}
		<img class="banner" src="{{image}}"/>
		{{else}}
		<img class="banner" src="{{defaultImage}}"/>
		{{/if}}
		<div class="col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
			<div class="section">
				<h1 class="title">{{title}}</h1>
				{{{content}}}
			</div>
		</div>
	</div>
</div>
{{/each}}
