
<div class="container full-width rounded">
	<div class="row">
		{{#if data.section.image.exists}}
		<img class="banner" src="{{data.section.image}}"/>
		{{else}}
		<img class="banner" src="{{data.section.defaultImage}}"/>
		{{/if}}
		<div class="col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
			<div class="section">
				<h1 class="title">{{data.section.title}}</h1>
				{{{data.section.content}}}
			</div>
		</div>
	</div>
</div>
